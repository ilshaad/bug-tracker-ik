{
  "name": "frontend-bug-tracker",
  "version": "1.0.0",
  "description": "frontend for the bug tracker",
  "private": true,
  "scripts": {
    "ikupdate": "npm i && npm update && npm audit fix --force && npm outdated",
    "build": "eslint . && rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest/* ./dist/ && webpack --config webpack.production.config.js",
    "prod": "npm run build && node src/server.js",
    "dev": "eslint . && rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest/* ./dist && webpack serve --config webpack.dev.config.js --hot",
    "server": "node src/server.js",
    "lint": "eslint .",
    "ikclean": "rm -fr node_modules/ package-lock.json dist/ && npm cache clean -vf && npm i && npm update && npm audit fix --force && npm outdated",
    "ikcleanci": "rm -fr node_modules/ dist/ && npm cache clean -vf && npm ci && npm outdated",
    "backend": "cd ../backend && npm run server"
  },
  "keywords": [],
  "author": "Rechad Salma",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@tsconfig/node16": "^1.0.3",
    "@types/react": "^18.0.25",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.5",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "handlebars-loader": "^1.7.2",
    "html-webpack-plugin": "^5.4.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "react-bootstrap": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "dotenv": "^16.0.3",
    "formik": "^2.2.9",
    "handlebars": "^4.7.7",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.4.3",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  }
}
