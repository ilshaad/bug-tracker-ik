{
  "name": "frontend-bug-tracker",
  "version": "1.0.0",
  "description": "frontend for the bug tracker",
  "private": true,
  "scripts": {
    "ikupdate": "npm i && npm update && npm audit fix --force && npm outdated",
    "build": "rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest/* ./dist/ && webpack --config webpack.production.config.js",
    "prod": "npm run build && node src/server.js",
    "dev": "rm -rfv ./dist && mkdir dist && cp -rv ./src/public/manifest/* ./dist && webpack serve --config webpack.dev.config.js --hot",
    "server": "node src/server.js",
    "lint": "eslint .",
    "ikclean": "rm -fr node_modules/ package-lock.json dist/ && npm cache clean -vf && npm i && npm update && npm audit fix --force && npm outdated",
    "ikcleanci": "rm -fr node_modules/ dist/ && npm cache clean -vf && npm ci && npm outdated",
    "backend": "cd ../backend && npm run server"
  },
  "keywords": [],
  "author": "Rechad Salma",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@tsconfig/node16": "^16.1.4",
    "@types/react": "^19.1.6",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^10.0.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.4",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.2.1",
    "react-bootstrap": "^2.10.10",
    "react-redux": "^9.2.0",
    "react-refresh": "^0.17.0",
    "sass": "^1.89.1",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0"
  },
