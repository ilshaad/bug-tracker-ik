    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "dotenv": "^16.5.0",
    "formik": "^2.4.6",
    "handlebars": "^4.7.8",
    "react": "^19.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^7.6.2",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  }
}
